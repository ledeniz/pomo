{
  "name": "pomodo.work",
  "version": "3.0.0",
  "description": "Pomodoro technique web-app",
  "engines": {
    "node": "6.7.0",
    "npm": "3.10.8"
  },
  "scripts": {
    "start": "webpack-dev-server --env.dev",
    "postinstall": "webpack --env.prod",
    "test": "cross-env NODE_ENV=test ava",
    "test:watch": "npm test -- -watch",
    "coverage": "nyc npm test",
    "check:src": "npm run lint && npm run test",
    "travis:coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomspeak/pomodo.work.git"
  },
  "author": "Tom Speak <hello@tomspeak.co.uk> (http://github.com/tomspeak)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomspeak/pomodo.work/issues"
  },
  "homepage": "http://pomodo.work",
  "ava": {
    "require": [
      "babel-register",
      "ignore-styles",
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit",
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "powerAssert": false,
    "verbose": true
  },
  "nyc": {
    "include": [
      "assets/js/**/*.{js,jsx}"
    ],
    "exclude": [
      "test/**/*.spec.js",
      "public/**/**",
      "node_modules/**/**"
    ],
    "require": [
      "babel-register",
      "ignore-styles",
      "./test/helpers/setup-browser-env.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "enzyme": "2.4.1",
    "expect": "^1.20.2",
    "expect-jsx": "^2.6.0",
    "ignore-styles": "5.0.1",
    "jsdom": "^9.5.0",
    "nyc": "^14.1.0",
    "react-hot-loader": "3.0.0-beta.5",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "1.2.1",
    "sinon": "1.17.6",
    "webpack": "^2.7.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "6.2.5",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-es2015-native-modules": "6.9.4",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.16.3",
    "classnames": "^2.2.5",
    "coveralls": "^3.0.3",
    "cross-env": "3.1.2",
    "css-loader": "^2.1.1",
    "postcss-loader": "^0.13.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-sounds": "^1.1.1",
    "redux-thunk": "^2.1.0",
    "store": "1.3.20",
    "style-loader": "^0.13.1",
    "webpack-config-utils": "^2.1.0"
  }
}
